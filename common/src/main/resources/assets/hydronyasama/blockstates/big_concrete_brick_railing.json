{
  "multipart": [
    {
      "apply": {
        "model": "hydronyasama:block/big_concrete_brick_railing_post"
      }
    },
    {
      "when": {
        "up": true
      },
      "apply": {
        "model": "hydronyasama:block/big_concrete_brick_railing_post_top"
      }
    },
    {
      "when": {
        "down": true
      },
      "apply": {
        "model": "hydronyasama:block/big_concrete_brick_railing_post_bottom"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "hydronyasama:block/big_concrete_brick_railing_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "hydronyasama:block/big_concrete_brick_railing_side",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "hydronyasama:block/big_concrete_brick_railing_side",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "hydronyasama:block/big_concrete_brick_railing_side",
        "y": 270,
        "uvlock": true
      }
    }
  ]
}
